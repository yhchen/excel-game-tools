{
    "files.watcherExclude": {
        "**/.git/**/*": true,
        "**/.git/objects/**": false,
        "**/.git/subtree-cache/**": false,
        "**/bin/**/*": true,
        "**/node_modules/**/*": true,
        "**/node_modules/*/**": false
    },
    "search.exclude": {
        "**/bin": true,
        "**/dist/**/*": true
    },
    "editor.insertSpaces": false,
    "files.exclude": {
        "**/*.js.map": true,
        "node_modules": true,
        "package-lock.json": true,
        "test-protobuf-net": true,
        "**/*.js": true,
        "package.json": true,
        ".vscode": true,
        "tsconfig.json": true,
    },
    "editor.snippetSuggestions": "bottom",
    "javascript.format.semicolons": "insert",
    "typescript.format.semicolons": "insert",
    "files.eol": "\n",
}